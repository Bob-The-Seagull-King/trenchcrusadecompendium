    {
        "id": "eq_machinegun",
        "type": "Equipment",
        "source": "core",
        "tags": [
                {"tag_name": "Heavy", "val": ""}
                ],
        "category": "ranged",
        "name": "Machine Gun",
        "equip_type": "2-Handed",
        "range": "36\"",
        "blurb": "Machine Guns are lethal fully-automatic firearms, capable of mowing down entire ranks of infantry from afar. The firing rate depends on the lock assembly used but averages 500 rounds per minute.",
        "modifiers": ["3 Attacks"],
        "description":  [
                            {
                                "tags": [{"tag_name": "desc_type", "val": "effect"}],
                                "content": "Rule:",
                                "subcontent": [
                                    {
                                        "tags": [{"tag_name": "desc_type", "val": "desc"}],
                                        "content": "Machine Guns can make three Attack Actions instead of one. They can target separate models with each attack, as long as all targets are within 6” of each other. After taking all three attacks the Activation of the model is over regardless of any remaining ACTIONS the model might have.",
                                        "glossary": [{"val": "ACTIONS", "id": "gl_action"}]
                                    }
                                ]
                            }
                        ]
    },
    {
        "id": "eq_flamethrower",
        "type": "Equipment",
        "source": "core",
        "tags": [
                {"tag_name": "Fire", "val": ""}
                ],
        "category": "ranged",
        "name": "Flamethrower",
        "equip_type": "2-Handed",
        "range": "8\"",
        "blurb": "A flamethrower is a terrifying weapon capable of projecting great streams of fire and flammable liquids at a distance. It is ideal for clearing bunkers, trenches and other fortifications, killing in a most horrific way. Consequently, it is greatly favoured by the Heretic forces.",
        "modifiers": ["-1D Injuries"],
        "description":  [
                            {
                                "tags": [{"tag_name": "desc_type", "val": "effect"}],
                                "content": "Rule:",
                                "subcontent": [
                                    {
                                        "tags": [{"tag_name": "desc_type", "val": "desc"}],
                                        "content": "A Flamethrower hits one model within range automatically with its attack ACTION, ignoring armour. Injuries caused by a Flamethrower are rolled with an additional -1 DICE. It also has the Keyword FIRE, so it causes an additional +1 BLOOD MARKER on enemies it hits.",
                                        "glossary": [{"val": "ACTION", "id": "gl_action"},{"val": "-1 DICE", "id": "gl_minusdice"},{"val": "FIRE", "id": "gl_fire"},{"val": "+1 BLOOD MARKER", "id": "gl_bloodmarker"}]
                                    }
                                ]
                            }
                        ]
    },
    {
